{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://llouisetaylor.github.io",
  "dependencies": {
    "express": "^4.16.3",
    "gh-pages": "^1.1.0",
    "morgan": "^1.9.1",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-inlinesvg": "^0.8.1",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17",
    "svg-inline-react": "^3.0.0"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -b master -d build",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js && copyfiles README.md build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build-css": "node-sass-chokidar src/ -o src/styles",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/styles --watch --recursive"
  },
  "devDependencies": {
    "copyfiles": "^1.2.0"
  }
}
